{
  "name": "platzigram-api",
  "version": "1.0.0",
  "description": "Platzigram REST API",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && NODE_ENV=test ava test/**-test.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cybermanpy/platzigram-api.git"
  },
  "author": "Diego Saldivar <fhersa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cybermanpy/platzigram-api/issues"
  },
  "homepage": "https://github.com/cybermanpy/platzigram-api#readme",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^9.0.0",
    "babel-register": "^6.26.0",
    "standard": "^12.0.1",
    "test-listen": "^1.1.0"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "http-hash": "^2.0.0",
    "jsonwebtoken": "^8.3.0",
    "micro": "^9.3.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "token-extractor": "^0.1.6",
    "uuid-base62": "^0.1.0"
  }
}
